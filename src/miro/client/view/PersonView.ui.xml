<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:miroView='urn:import:miro.client.view'>

	<!--
		unit indique les unités utilisées pour exprimer la taille des
		composants que contient le DockLayout
	-->
	<g:DockLayoutPanel unit='EM'>
		<g:north size="8">
			<miroView:TopPanel />
		</g:north>
		<g:center>
			<miroView:CenterPanel />
		</g:center>
		<g:south size="9">
			<g:Image url="cirb-brussels-it-is-for-you-footer.png" />
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>